{
  "id": "pwadevday",
  "scope": "/",
  "name": "示例 - 中国 PWA 开发者日",
  "display": "standalone",
  "display_override": ["window-controls-overlay"],
  "start_url": "/",
  "short_name": "示例 - 中国 PWA 开发者日",
  "theme_color": "#ffffff",
  "description": "中国 PWA 开发者日示例",
  "orientation": "any",
  "background_color": "#ffffff",
  "related_applications": [],
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "assets/icons/icon_512.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "assets/icons/icon_192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "assets/icons/icon_32.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "assets/icons/icon_16.png",
      "sizes": "16x16",
      "type": "image/png"
    }
  ],
  "protocol_handlers": [{
    "protocol": "web+pwadev",
    "url": "/sample/url-protocol-handler?pwadev=%s"
  }],
  "file_handlers": [
    {
      "action": "/sample/file-handling",
      "accept": {
        "image/webp": [".webp"],
        "image/jpeg": [".jpeg", ".jpg"],
        "image/png": [".png"]
      },
      "launch_type": "single-client"
    },
    {
      "action": "/sample/media-player",
      "accept": {
        "video/*": [".mp4",".mkv",".webm",".3gp",".avi",".mpeg",".mpg",".ogg",".mp3",".wmv",".wav",".mov"],
        "audio/*": [".mp4",".mkv",".webm",".3gp",".avi",".mpeg",".mpg",".ogg",".mp3",".wmv",".wav",".mov"]
      },
      "launch_type": "single-client"
    }
  ],
  "screenshots": [
    {
      "src": "assets/screenshots/screen.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "features": [
    "渐进式 Web 应用 (PWA)",
    "渐进式 Web 应用 (PWA) 示例",
    "本地视频播放器"
  ],
  "categories": [
    "utility"
  ],
  "shortcuts": [
    {
      "name": "首页",
      "short_name": "Home",
      "description": "打开示例首页",
      "url": "/",
      "icons": [{ "src": "assets/icons/icon_192.png", "sizes": "192x192" }]
    },
    {
      "name": "本地视频播放器",
      "short_name": "Sample",
      "description": "打开本地视频播放器",
      "url": "/sample/media-player",
      "icons": [{ "src": "assets/icons/icon_192.png", "sizes": "192x192" }]
    }
  ]
}
